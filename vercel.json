{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/shopify/products/[id].ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}